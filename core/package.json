{
  "name": "@ladesa-ro/especificacao-core",
  "version": "2.0.0-next.5",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "release:base": "release-it -c .release-it.json",
    "build": "unbuild"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "files": [
    "*.md",
    "proto/**/*.*",
    "dist/**/*.*",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "optionalDependencies": {
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "unbuild": "^2.0.0"
