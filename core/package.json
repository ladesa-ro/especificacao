{
  "name": "@sisgea/spec",
  "version": "1.1.0-development.63",
  "main": "dist/index.js",
  "sideEffects": false,
  "publishConfig": {
    "provenance": true
  },
  "files": [
    "*.md",
    "proto/**/*.*",
    "dist/**/*.*",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "prepare": "ts-patch install -s",
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "docs": "rimraf docs; typedoc --maxTypeConversionDepth 10 --out  docs src/index.ts",
    "format:write": "prettier --write --no-error-on-unmatched-pattern \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.8",
    "ts-patch": "^3.1.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.7"
  },
  "optionalDependencies": {
    "yup": "^1.4.0"
  },
  "types": "./dist/index.d.ts",
  "description": "## Instalação",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sisgha/spec.git"
  },
  "keywords": [
    "sisgha",
    "sisgea",
    "specification"
  ],
  "author": "Gabriel R. Antunes <gabrielrodantunes@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sisgha/spec/issues"
  },
  "homepage": "https://github.com/sisgha/spec#readme",
  "dependencies": {
    "@unispec/compiler": "^0.8.0",
    "@unispec/core": "^0.11.0"
  }
}
